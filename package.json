{
  "name": "weconn-proper",
  "version": "1.0.0",
  "description": "an api for the weconnect platform ",
  "main": "app.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.9.0",
    "nyc": "^11.6.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.1"
  },
  "devDependencies": {
    "mocha": "^5.0.4"
  },
  "scripts": {
    "pre-localTest": "sequelize db:migrate:undo:all --env localTest && sequelize db:migrate --env localTest && cls",
    "localTest": "cross-env NODE_ENV=localTest nyc mocha ./test --compilers js:babel-core/register --timeout 15000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "start": "nodemon --exec babel-node ./server/bin/www",
    "test": "cross-env NODE_ENV=localTest nyc mocha ./test --compilers js:babel-core/register --timeout 15000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewinsoul/WEConnect-proper.git"
  },
  "keywords": [
    "api",
    "weconnect"
  ],
  "author": "Andy",
